#!/bin/bash
rm -rf target
cd /workspace/examples/digital\ twins/drobotti_rmqfmu

printf "Remove generated files and folders"
rm -rf target
rm out.txt
rm -rf generate
rm publisher-out.txt
rm consume.txt

printf "Kill background processes (publisher and consume scripts)"
kill $(ps -ef | grep "rmq-publisher.py" | awk '{print $2}')

kill $(ps -ef | grep "consume.py" | awk '{print $2}')
